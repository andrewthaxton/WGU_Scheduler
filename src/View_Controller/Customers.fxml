<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="425.0" prefWidth="646.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.CustomersController">
   <children>
      <Label fx:id="customersTitleLabel" layoutX="288.0" layoutY="29.0" text="Customers">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TableView fx:id="customersTableview" layoutX="8.0" layoutY="80.0" prefHeight="252.0" prefWidth="631.0">
        <columns>
          <TableColumn fx:id="customerIdCol" prefWidth="75.0" text="Customer ID" />
          <TableColumn fx:id="nameCol" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="addressCol" prefWidth="90.0" text="Address" />
            <TableColumn fx:id="postalCol" prefWidth="75.0" text="Postal" />
            <TableColumn fx:id="phoneCol" prefWidth="75.0" text="Phone" />
            <TableColumn fx:id="divisionIdCol" prefWidth="75.0" text="Division ID" />
            <TableColumn fx:id="divisionNameCol" prefWidth="90.0" text="Division Name" />
            <TableColumn fx:id="countryCol" prefWidth="75.0" text="Country" />
        </columns>
      </TableView>
      <Button fx:id="addCustomerButton" layoutX="39.0" layoutY="359.0" mnemonicParsing="false" onAction="#onAddCustomerButton" text="Add" />
      <Button fx:id="updateCustomerButton" layoutX="222.0" layoutY="359.0" mnemonicParsing="false" onAction="#onUpdateCustomerButton" text="Update" />
      <Button fx:id="deleteCustomerButton" layoutX="404.0" layoutY="359.0" mnemonicParsing="false" onAction="#onDeleteCustomerButton" text="Delete" />
      <Button fx:id="homeButton" layoutX="556.0" layoutY="359.0" mnemonicParsing="false" onAction="#onHomeButton" text="Home" />
   </children>
</AnchorPane>
