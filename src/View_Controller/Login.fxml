            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label fx:id="usernameLabel" layoutX="41.0" layoutY="94.0" text="Username" />
      <Label fx:id="passwordLabel" layoutX="43.0" layoutY="133.0" text="Password" />
      <Label fx:id="zoneIDLabel" layoutX="176.0" layoutY="216.0" text="ZoneID" />
      <TextField fx:id="usernameTextbox" layoutX="122.0" layoutY="90.0" />
      <TextField fx:id="passwordTextbox" layoutX="122.0" layoutY="129.0" />
      <Button fx:id="loginButton" layoutX="170.0" layoutY="172.0" mnemonicParsing="false" onAction="#onLoginButton" text="Login" />
   </children>
</AnchorPane>
