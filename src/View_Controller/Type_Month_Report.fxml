            <TableColumn fx:id="numAptCol" prefWidth="120.0" text="# of Appointments" />
        </columns>
      </TableView>
      <Button fx:id="exitButton" layoutX="283.0" layoutY="349.0" mnemonicParsing="false" onAction="#onExitButton" text="Exit" />
   </children>
</AnchorPane>
